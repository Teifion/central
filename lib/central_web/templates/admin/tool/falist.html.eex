<style type="text/css">
  .icon-row {
    margin-bottom: 30px;
  }

  .icons {
    letter-spacing: 2em;
    display: inline-block;
  }

  .icon-name {
    padding-left: 15px;
    font-size: 30px;
  }
</style>

<div class="row">
  <div class="col-md-8">
    <h4>Standard</h4>
    <%= for _ <- 1..12 do %>
      <% the_icon = Enum.random(standard) %>
      <div class="icon-row">
        <div class="icons">
          <i class="fal fa-fw fa-3x fa-<%= the_icon %>"></i>
          <i class="far fa-fw fa-3x fa-<%= the_icon %>"></i>
          <i class="fas fa-fw fa-3x fa-<%= the_icon %>"></i>
          <i class="fad fa-fw fa-3x fa-<%= the_icon %>"></i>
        </div>
        <span class="icon-name">
          <%= the_icon %>
        </span>
      </div>
    <% end %>
  </div>
  <div class="col-md-4">
    <h4>Brand</h4>
    <%= for _ <- 1..12 do %>
      <% the_icon = Enum.random(brand) %>
      <div class="icon-row">
        <i class="fab fa-fw fa-3x fa-<%= the_icon %>"></i>
        <span class="icon-name">
          <%= the_icon %>
        </span>
      </div>
    <% end %>
  </div>
</div>
